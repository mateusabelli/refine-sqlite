{
  "name": "refine-sqlite",
  "description": "SQLite data provider for the refine framework",
  "version": "0.1.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "Mateus Abelli",
  "keywords": [
    "refine",
    "data provider",
    "sqlite"
  ],
  "private": false,
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mateusabelli/refine-sqlite.git"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/mateusabelli"
    }
  ],
  "engines": {
    "node": ">=16"
  },
  "module": "dist/esm/index.js",
  "scripts": {
    "start": "tsup --watch --format esm,cjs,iife --legacy-output",
    "build": "tsup --format esm,cjs,iife --minify --legacy-output",
    "test": "jest --passWithNoTests --runInBand",
    "prepare": "npm run build"
  },
  "dependencies": {
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@esbuild-plugins/node-resolve": "^0.2.2",
    "@refinedev/core": "^4.45.1",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  }
}
